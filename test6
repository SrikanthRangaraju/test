SELECT 
    a.rut,
    ab.bic_cant_doc_3um,
    am.mcc_cant_doc_3um,
    am.mcc_avg_monto,
    am.mcc_max_monto,
    amtu.mtu_cant_doc_3um
FROM 
    attr_bic ab
INNER JOIN 
    attr_mcc am ON ab.rut = am.rut
INNER JOIN 
    attr_mtu amtu ON ab.rut = amtu.rut
INNER JOIN 
    chile_bic cb ON ab.rut = cb.rut
INNER JOIN 
    chile_mcc cm ON ab.rut = cm.rut
INNER JOIN 
    chile_mtu cmtu ON ab.rut = cmtu.rut
WHERE 
    ab.date = 202311
    AND am.date = 202311
    AND amtu.date = 202311
    AND cb.date = 202311
    AND cm.date = 202311
    AND cmtu.date = 202311;
