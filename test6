 # Process the response file
    if [ -s SomeResponse/"$SomeIDS".tmp ]; then
      tail -n +2 SomeResponse/"$SomeIDS".tmp | head -n -1 | sed 's/"response": {/{/' > SomeResponse/"$SomeIDS".json
      rm SomeResponse/"$SomeIDS".tmp
    else
      echo "Error: Empty response for $SomeIDS" >&2
      rm -f SomeResponse/"$SomeIDS".tmp
    fi
  done < testData.txt

  # ZIP the SomeResponse folder with date-stamped name
  zip_name="SomeResponse_$(date +%d%m%Y).zip"
  zip -r "$zip_name" SomeResponse

  # Copy to target directory (adjust paths as needed)
  cp "$zip_name" /tmp/test/
  cp "$zip_name" /path/to/target/directory/  # Replace with your actual target path
' > output.log 2>&1 &
