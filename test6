DO $$ 
DECLARE 
    r RECORD;
BEGIN
    FOR r IN (SELECT tablename FROM pg_tables WHERE schemaname = 'icrsmismatch') 
    LOOP
        EXECUTE 'DROP TABLE icrsmismatch.' || r.tablename || ' CASCADE';
    END LOOP;
END $$;


pg_dump -U your_user -h your_host -d your_database -n icrsmismatchalgos | psql -U your_user -h your_host -d your_database -n icrsmismatch
