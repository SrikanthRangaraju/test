SELECT 
    a.rut,
    a.bic_cant_doc_3um,
    a.ddmo_bic,
    b.mtu_cant_doc_3um,
    b.ddm0_mtu
FROM 
    attr_bic a
INNER JOIN 
    attr_mtu b ON a.rut = b.rut
INNER JOIN 
    chile_bic c ON a.rut = c.rut
INNER JOIN 
    chile_mtu d ON a.rut = d.rut
WHERE 
    a.date = 202311 
    AND b.date = 202311
    AND c.date = 202311
    AND d.date = 202311;
